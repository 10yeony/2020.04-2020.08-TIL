package self.test;

import java.util.Scanner;

public class PatternGameTest {

	public static void main(String[] args) {

		System.out.println("10~99 사이의 정수를 입력하세요>>>");
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		// % , / 연산자를 이용
		for (int run = 0;;) {
			if (num >= 10 && num <= 99) {
				run = 1;
			} else {
				run = 0;
			}
		}

	}

}
