package self;

public class Programmer {
	private String name;
	private int salary;
	private String tech;
	public MyDate birthDay;
	
	public Programmer(String name, int salary, String tech, public MyDate birthDay) {
		super();
		this.name = name;
		this.salary = salary;
		this.tech = tech;
		this.birthDay = birthDay;
	}
	
	public NoteBook noteBook;
	public Programmer(NoteBook noteBook) {
		super();
		this.noteBook = noteBook;
	}
	
}