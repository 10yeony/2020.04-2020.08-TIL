		for (int run = 0;;) {
			System.out.println("10~99 사이의 정수를 입력하세요>>>");
			Scanner sc = new Scanner(System.in);
			int num = sc.nextInt();
			// % , / 연산자를 이용
			if (num >= 10 && num <= 99) {
				int quotient = num / 10;
				int remainder = num % 10;
				run = 1;
				if(num % 3 == 0) {
					
				}
			} else {
				run = 0;
			}
		}

	}

}
