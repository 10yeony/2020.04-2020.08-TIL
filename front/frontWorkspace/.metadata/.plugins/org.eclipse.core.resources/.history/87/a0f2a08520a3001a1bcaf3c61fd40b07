package practice;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int total = (n-1)*2+1;
		for(int i=0; i<total; i++) {
			for() {
				System.out.printf(" ");
			}
			for() {
				System.out.printf("*");
			}
			for() {
				System.out.printf(" ");
			}
			System.out.println();
		}
	}
}