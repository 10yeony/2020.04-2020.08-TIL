package practice;

import java.util.Arrays;

public class Practice {
	public static void main(String[] args) {
		int n = 3;
		int answer = 0;
        int[] fList = new int[n+1];
        fList[0] = 0;
        fList[1] = 1;
        for(int i=2; i<n+1; i++){
            fList[i] = fList[i-1] + fList[i-2];
        }
        answer = fList[n] % 1234567;
        
        System.out.println(answer);
	}

}
