package practice;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		int count = 0;
		int temp = -1;
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		while(temp!=n) {
			temp = ((n/10) + (n%10))%10 + n%10*10;
			count += 1;
		}
		System.out.println(count);
	}
}