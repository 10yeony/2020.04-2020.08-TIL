package com.encore.spring.model.impl;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.encore.spring.domain.Member;
import com.encore.spring.model.MemberDAO;

@Repository
public class MemberDAOImpl implements MemberDAO {
	
	@Autowired
	SqlSession sqlSession;
	
	@Override
	public List<Member> viewAllMember() {
		return null;
	}

	@Override
	public Member findMember(String id) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void registerMember(Member vo) {
		sqlSession.insert("MemberMapper.registerMember", vo);
		sqlSession.commit();
		
	}

	@Override
	public void updateMember(Member vo) {
		sqlSession.update("MemberMapper.updateMember", vo);
		sqlSession.commit();
	}

}
