package com.encore.spring.domain;

public class Member {
	private String id;
	private String pwd;
	private String name;
	private String address;
	
	public Member() {}
	public Member(String id, String pwd, String name, String address) {
		super();
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.address = address;
	}
	
	
}
