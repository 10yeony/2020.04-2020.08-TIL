package oop.greet;
// EncoreGreet 클래스의 용도...
/*
* EncoreGreet는 
* Encore 교육센터 수강생들에게 전하는 인사말을 저장하고 있는 클래스
 */
public class EncoreGreet {
	// Variable(변수) : 값이 저장되는 공간
	public String name = "James";
	public String message = "Hello Good Day~~~";
	
	public void sayHello() {
		System.out.println(name+", "+message);
	}

}
