package array.test2;

public class ArrayMaxAndMinTest2 {

	public static void main(String[] args) {
		int[ ] score = {79,88,91,44,100,55,95};
		
		int min = score[0]; // 배열의 첫번째 값으로 최소값을 초기화 한다... 79
		int max = score[0]; // 배열의 첫번째 값으로 최대값을 초기화한다... 100
		
		/*for(int i=0; i<7; i++) {
			if(score[i]<min) min = score[i];
			if(score[i]>max) max = score[i];
		}*/
		// 알고리즘 풀 때는 이렇게 푸는데(알고리즘은 원시적으로)
		// 가장 원시적인 언어가 C, 그 다음 자바, 파이썬. (파이썬은 편해보이지만 메모리 많이 잡아먹음)
		
		// Advanced For문... ForEach
		// 
		
		
		System.out.println("최소값 : " + min);
		System.out.println("최대값 : " + max);
	}

}
